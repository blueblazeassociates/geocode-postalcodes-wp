{
  "name" : "blueblazeassociates/geocode-postalcodes-wp",
  "description" : "WordPress integration for blueblazeassociates/geocode-postalcodes.",
  "type" : "wordpress-plugin",
  "homepage" : "https://github.com/blueblazeassociates/geocode-postalcodes-wp",
  "license" : "GPL-2.0+",
  "authors": [{
      "name" : "Ed Gifford",
      "homepage" : "http://www.blueblazeassociates.com",
      "email" : "egifford@blueblazeassociates.com"
  }],
  "keywords" : [
    "geocode",
    "geocoding",
    "zipcode",
    "zip code",
    "latitude",
    "longitude"
  ],
  "minimum-stability": "stable",
  "require": {
    "php" : ">=5.4",
    "blueblazeassociates/geocode-postalcodes" : "^0.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0.0"
  },
  "autoload" : {
    "psr-4" : {
      "BlueBlazeAssociates\\Geocoding\\" : "src/php"
    }
  },
  "extra" : {
    "coding-standard" : "Using WordPress PHP standard but using spaces for indentation instead of tabs."
  }
}
